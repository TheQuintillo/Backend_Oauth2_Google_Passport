{
  "name": "nodemon",
  "version": "1.0.0",
  "description": "Backend ",
  "main": "src/index.ts",
  "scripts": {
    "ts-node": "ts-node index.ts",
    "node": "nodemon ./src/server.ts"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.42",
    "jest": "^28.1.1",
    "nodemon": "^2.0.16",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "keywords": [
    "NodeJS",
    "TypeScript",
    "ExpressJS"
  ],
  "author": "The Quintillo",
  "license": "ISC",
  "_moduleAliases": {
    "@src": "./src"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "moduleNameMapper": {
      "@src/(.*)": "<rootDir>/src/$1",
      "@routes/(.*)": "<rootDir>/src/routes/$1"
    }
  },
  "dependencies": {
    "express": "^4.18.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "node": "^17.7.2"
  }
}
