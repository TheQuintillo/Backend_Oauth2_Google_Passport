{
  "name": "nodemon",
  "version": "1.0.0",
  "description": "Backend ",
  "main": "src/index.ts",
  "scripts": {
    "ts-node": "ts-node index.ts",
    "node": "nodemon ./src/server.ts"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^28.1.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.42",
    "@types/passport": "^1.0.9",
    "@types/passport-google-oauth": "^1.0.42",
    "jest": "^28.1.1",
    "nodemon": "^2.0.16",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "keywords": [
    "NodeJS",
    "TypeScript",
    "ExpressJS"
  ],
  "author": "The Quintillo",
  "license": "ISC",
  "_moduleAliases": {
    "@src": "./src",
    "@routes": "./src/routes",
    "@services": "./src/services",
    "@controllers": "./src/controllers"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "moduleNameMapper": {
      "@src/(.*)": "<rootDir>/src/$1",
      "@routes/(.*)": "<rootDir>/src/routes/$1",
      "@services/(.*)": "<rootDir>/src/services/$1",
      "@controllers/(.*)": "<rootDir>/src/services/$1"
    }
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "dotenv-expand": "^8.0.3",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "node": "^17.7.2",
    "passport": "^0.6.0",
    "passport-google-oauth": "^2.0.0"
  }
}
